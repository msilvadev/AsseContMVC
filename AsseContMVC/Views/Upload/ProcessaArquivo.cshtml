@model IEnumerable<AsseContMVC.Models.Marcacao>
@{
    ViewBag.Title = "Index";
}
<h2>Dados Processados</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nsr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoRegistro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataMarcacao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HoraMarcacao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pis)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nsr)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoRegistro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataMarcacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoraMarcacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pis)
            </td>
        </tr>
    }

</table>
